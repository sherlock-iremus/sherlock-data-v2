n": [{"id": "/div[1]/quote[1]/l[11]/seg[1]/text()[1]", "text": "    "}], "attributes": {"type": "tab"}}, {"id": "/div[1]/quote[1]/l[11]/text()[1]", "text": "Bacchus, & l’autre c’est l’Amour."}]}, {"id": "/div[1]/quote[1]/l[12]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[12]/text()[1]", "text": "Consolez-vous, Divinitez puissantes,"}]}, {"id": "/div[1]/quote[1]/l[13]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[13]/seg[1]", "tag": "seg", "children": [{"id": "/div[1]/quote[1]/l[13]/seg[1]/text()[1]", "text": "    "}], "attributes": {"type": "tab"}}, {"id": "/div[1]/quote[1]/l[13]/text()[1]", "text": "Leur disoit le Destin tout-bas,"}]}, {"id": "/div[1]/quote[1]/l[14]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[14]/text()[1]", "text": "Si la charmante Iris préfere à vos appas"}]}, {"id": "/div[1]/quote[1]/l[15]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[15]/text()[1]", "text": "L’honneur d’estre comptée au nombre des Sçavantes,"}]}, {"id": "/div[1]/quote[1]/l[16]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[16]/seg[1]", "tag": "seg", "children": [{"id": "/div[1]/quote[1]/l[16]/seg[1]/text()[1]", "text": "    "}], "attributes": {"type": "tab"}}, {"id": "/div[1]/quote[1]/l[16]/text()[1]", "text": "C’est qu’elle ne vous connoist pas."}]}, {"id": "/div[1]/quote[1]/l[17]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[17]/seg[1]", "tag": "seg", "children": [{"id": "/div[1]/quote[1]/l[17]/seg[1]/text()[1]", "text": "    "}], "attributes": {"type": "tab"}}, {"id": "/div[1]/quote[1]/l[17]/text()[1]", "text": "Hé, comment s’en faire connoistre,"}]}, {"id": "/div[1]/quote[1]/l[18]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[18]/seg[1]", "tag": "seg", "children": [{"id": "/div[1]/quote[1]/l[18]/seg[1]/text()[1]", "text": "    "}], "attributes": {"type": "tab"}}, {"id": "/div[1]/quote[1]/l[18]/text()[1]", "text": "Répondit Bacchus irrité ?"}]}, {"id": "/div[1]/quote[1]/l[19]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[19]/text()[1]", "text": "Je ne puis approcher cette fiere Beauté ;"}]}, {"id": "/div[1]/quote[1]/l[20]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[20]/seg[1]", "tag": "seg", "children": [{"id": "/div[1]/quote[1]/l[20]/seg[1]/text()[1]", "text": "    "}], "attributes": {"type": "tab"}}, {"id": "/div[1]/quote[1]/l[20]/text()[1]", "text": "D’abord qu’elle me voit paroistre,"}]}, {"id": "/div[1]/quote[1]/l[21]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[21]/seg[1]", "tag": "seg", "children": [{"id": "/div[1]/quote[1]/l[21]/seg[1]/text()[1]", "text": "    "}], "attributes": {"type": "tab"}}, {"id": "/div[1]/quote[1]/l[21]/text()[1]", "text": "Qu’on le jette par la fenestre,"}]}, {"id": "/div[1]/quote[1]/l[22]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[22]/seg[1]", "tag": "seg", "children": [{"id": "/div[1]/quote[1]/l[22]/seg[1]/text()[1]", "text": "    "}], "attributes": {"type": "tab"}}, {"id": "/div[1]/quote[1]/l[22]/text()[1]", "text": "Dit-elle, & qu’on ne laisse pas"}]}, {"id": "/div[1]/quote[1]/l[23]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[23]/seg[1]", "tag": "seg", "children": [{"id": "/div[1]/quote[1]/l[23]/seg[1]/text()[1]", "text": "    "}], "attributes": {"type": "tab"}}, {"id": "/div[1]/quote[1]/l[23]/text()[1]", "text": "La moindre trace de ses pas."}]}, {"id": "/div[1]/quote[1]/l[24]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[24]/seg[1]", "tag": "seg", "children": [{"id": "/div[1]/quote[1]/l[24]/seg[1]/text()[1]", "text": "    "}], "attributes": {"type": "tab"}}, {"id": "/div[1]/quote[1]/l[24]/text()[1]", "text": "Helas ! ma fortune est pareille,"}]}, {"id": "/div[1]/quote[1]/l[25]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[25]/seg[1]", "tag": "seg", "children": [{"id": "/div[1]/quote[1]/l[25]/seg[1]/text()[1]", "text": "    "}], "attributes": {"type": "tab"}}, {"id": "/div[1]/quote[1]/l[25]/text()[1]", "text": "Dit languissamment à son tour"}]}, {"id": "/div[1]/quote[1]/l[26]", 