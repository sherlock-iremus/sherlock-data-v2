l[4]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[4]/text()[1]", "text": "Je crains tout, mais enfin je ne le pense pas."}], "attributes": {"rend": "i"}}]}, {"id": "/div[1]/quote[1]/ab[2]", "tag": "ab", "children": [{"id": "/div[1]/quote[1]/ab[2]/text()[1]", "text": "***"}], "attributes": {"type": "ornament"}}, {"id": "/div[1]/quote[1]/lg[3]", "tag": "lg", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[1]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[1]/text()[1]", "text": "Vous estes descendu d’une Source trop pure,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[3]/l[2]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[2]/text()[1]", "text": "  Pour ternir par cette action"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[3]/l[3]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[3]/text()[1]", "text": "  Vostre crystal & vostre nom ;"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[3]/l[4]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[4]/text()[1]", "text": "  Et si j’en croy vostre murmure,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[3]/l[5]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[5]/text()[1]", "text": "Vous ne serez jamais inconstant ny parjure."}], "attributes": {"rend": "i"}}]}, {"id": "/div[1]/quote[1]/ab[3]", "tag": "ab", "children": [{"id": "/div[1]/quote[1]/ab[3]/text()[1]", "text": "***"}], "attributes": {"type": "ornament"}}, {"id": "/div[1]/quote[1]/lg[4]", "tag": "lg", "children": [{"id": "/div[1]/quote[1]/lg[4]/l[1]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[4]/l[1]/text()[1]", "text": "  Cependant la rapidité"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[4]/l[2]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[4]/l[2]/text()[1]", "text": "Dont je vous voy courir le long de ce rivage,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[4]/l[3]", "tag": "l", "children": [