: [{"id": "/div[1]/quote[1]/lg[1]/l[20]/hi[1]/text()[1]", "text": "Je vous verray trompé, Seigneur, & moy confus."}], "attributes": {"rend": "i"}}]}, {"id": "/div[1]/quote[1]/lg[1]/l[21]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[21]/hi[1]", "tag": "hi", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[21]/hi[1]/text()[1]", "text": "J’ay, si vous le voulez, une grande ressource ;"}], "attributes": {"rend": "i"}}]}, {"id": "/div[1]/quote[1]/lg[1]/l[22]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[22]/hi[1]", "tag": "hi", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[22]/hi[1]/text()[1]", "text": "Vous pouvez me nommer pour Iuge de la Course,"}], "attributes": {"rend": "i"}}]}, {"id": "/div[1]/quote[1]/lg[1]/l[23]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[23]/hi[1]", "tag": "hi", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[23]/hi[1]/text()[1]", "text": "Pour Maréchal de Camp, pour ce qu’il vous plaira,"}], "attributes": {"rend": "i"}}]}, {"id": "/div[1]/quote[1]/lg[1]/l[24]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[24]/hi[1]", "tag": "hi", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[24]/hi[1]/text()[1]", "text": "Hors d’estre Combatant, tout Nom me conviendra."}], "attributes": {"rend": "i"}}]}, {"id": "/div[1]/quote[1]/lg[1]/l[25]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[25]/hi[1]", "tag": "hi", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[25]/hi[1]/text()[1]", "text": "Mais si je trouve en Vous un Prince inéxorable,"}], "attributes": {"rend": "i"}}]}, {"id": "/div[1]/quote[1]/lg[1]/l[26]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[26]/hi[1]", "tag": "hi", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[26]/hi[1]/text()[1]", "text": "Et si je n’obtiens rien qui me soit favorable,"}], "attributes": {"rend": "i"}}]}, {"id": "/div[1]/quote[1]/lg[1]/l[27]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[27]/hi[1]", "tag": "hi", "children": [{"id": "/div[1]/qu