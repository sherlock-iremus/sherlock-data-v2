{"id:": "/div[1]", "tag": "div", "attributes": {"type": "article", "{http://www.w3.org/XML/1998/namespace}id": "MG-1705-12_418", "resp": "angele"}, "children": [{"id": "/div[1]/head[1]", "tag": "head", "children": [{"id": "/div[1]/head[1]/text()[1]", "text": "Air nouveau"}]}, {"id": "/div[1]/bibl[1]", "tag": "bibl", "children": [{"id": "/div[1]/bibl[1]/title[1]", "tag": "title", "children": [{"id": "/div[1]/bibl[1]/title[1]/text()[1]", "text": "Mercure galant"}]}, {"id": "/div[1]/bibl[1]/text()[1]", "text": ", décembre 1705 [tome 12], p. 418-419."}]}, {"id": "/div[1]/p[1]", "tag": "p", "children": [{"id": "/div[1]/p[1]/text()[1]", "text": "La Chanson que vous allez voir vous paroistra divertissante."}]}, {"id": "/div[1]/quote[1]", "tag": "quote", "children": [{"id": "/div[1]/quote[1]/label[1]", "tag": "label", "children": [{"id": "/div[1]/quote[1]/label[1]/text()[1]", "text": "AIR NOUVEAU."}]}, {"id": "/div[1]/quote[1]/note[1]", "tag": "note", "children": [{"id": "/div[1]/quote[1]/note[1]/text()[1]", "text": "Avis pour placer les Figures. L’Air, "}, {"id": "/div[1]/quote[1]/note[1]/hi[1]", "tag": "hi", "children": [{"id": "/div[1]/quote[1]/note[1]/hi[1]/text()[1]", "text": "Que j'étois sot qu[and] j'étois amoureux"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/note[1]/text()[2]", "text": ", [page] 418."}], "attributes": {"resp": "author", "place": "margin"}}, {"id": "/div[1]/quote[1]/lg[1]", "tag": "lg", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[1]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[1]/text()[1]", "text": "Que j'étois sot, quand j'étois amoureux !"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[2]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[2]/text()[1]", "text": "Souvent je répandois des larmes ;"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[3]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[3]/text()[1]", "text": "Mes plus beaux jours, mes jours les plus heureux,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[4]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[4]/text()[1]", "text": "Ne se passoient pas sans allarmes."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[5]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[5]/text()[1]", "text": "Mais depuis que Bachus m'a muni de ses armes,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[6]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[6]/text()[1]", "text": "Doux effet de ce jus divin !"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[7]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[7]/text()[1]", "text": "La joye a sçeu bannir de mon cœur le chagrin ;"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[8]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[8]/text()[1]", "text": "Je resiste à l'Amour, je méprise ses charmes."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[9]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[9]/text()[1]", "text": "Ah ! qu'un homme est heureux de n'aimer que le vin !"}], "attributes": {"rend": "i"}}]}]}, {"id": "/div[1]/ref[1]", "tag": "ref", "children": [{"id": "/div[1]/ref[1]/figure[1]", "tag": "figure", "children": [{"id": "/div[1]/ref[1]/figure[1]/graphic[1]", "tag": "graphic", "children": [], "attributes": {"url": "images/1705-12_418.JPG"}}]}], "attributes": {"target": "images/1705-12_418.JPG"}}]}