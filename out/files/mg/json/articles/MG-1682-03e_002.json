lg[1]/l[2]", "tag": "l", "children": [{"id": "/div[1]/quote[2]/lg[1]/l[2]/text()[1]", "text": " Point de pardon, point de clémence."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[2]/lg[1]/l[3]", "tag": "l", "children": [{"id": "/div[1]/quote[2]/lg[1]/l[3]/text()[1]", "text": "Vangez-vous, punissez cet infidelle Amant,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[2]/lg[1]/l[4]", "tag": "l", "children": [{"id": "/div[1]/quote[2]/lg[1]/l[4]/text()[1]", "text": "Qui peut vous oublier pour quelque temps d’absence."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[2]/lg[1]/l[5]", "tag": "l", "children": [{"id": "/div[1]/quote[2]/lg[1]/l[5]/text()[1]", "text": " Qui cesse d’aimer un moment,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[2]/lg[1]/l[6]", "tag": "l", "children": [{"id": "/div[1]/quote[2]/lg[1]/l[6]/text()[1]", "text": " Mérite un cruel châtiment."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[2]/lg[1]/l[7]", "tag": "l", "children": [{"id": "/div[1]/quote[2]/lg[1]/l[7]/text()[1]", "text": "Par de foibles raisons, il vous demande grace,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[2]/lg[1]/l[8]", "tag": "l", "children": [{"id": "/div[1]/quote[2]/lg[1]/l[8]/text()[1]", "text": "Hé, voulez-vous encore appuyer son audace,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[2]/lg[1]/l[9]", "tag": "l", "children": [{"id": "/div[1]/quote[2]/lg[1]/l[9]/text()[1]", "text": " En luy pardonnant lâchement."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[2]/lg[1]/l[10]", "tag": "l", "children": [{"id": "/div[1]/quote[2]/lg[1]/l[10]/text()[1]", "text": " Mais quand on aime fortement,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[2]/lg[1]/l[11]", "tag": "l", "children": [{"id": "/div[1]/quote[2]/lg[1]/l[11]/text()[1]", "text": " Helas, pour peu qu’un Amant fasse,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[2]/lg[1]/l[12]", "tag": "l", "children": [{"id": "/div[1]/quote[2]/lg[1]/l[12]/tex