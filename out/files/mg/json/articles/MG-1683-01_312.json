utes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/ab[3]", "tag": "ab", "children": [{"id": "/div[1]/quote[1]/lg[1]/ab[3]/text()[1]", "text": "***"}], "attributes": {"type": "ornament"}}, {"id": "/div[1]/quote[1]/lg[1]/l[16]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[16]/text()[1]", "text": "La moitié de moy-mesme,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[17]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[17]/text()[1]", "text": "Est seûrement digne qu'on l'aime ;"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[18]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[18]/text()[1]", "text": "Car sans son utile secours"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[19]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[19]/text()[1]", "text": "On ne parleroit plus d'amours,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[20]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[20]/text()[1]", "text": "Et les Femmes seroient chargées"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[21]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[21]/text()[1]", "text": "Dés le mesme moment en Fées."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/ab[4]", "tag": "ab", "children": [{"id": "/div[1]/quote[1]/lg[1]/ab[4]/text()[1]", "text": "***"}], "attributes": {"type": "ornament"}}, {"id": "/div[1]/quote[1]/lg[1]/l[22]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[22]/text()[1]", "text": "L'autre moitié mérite encore plus d'honneur."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[23]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[23]/text()[1]", "text": "De la Divinité c'est une ample partie."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[24]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[24]/text()[1]", "text": "Elle a de