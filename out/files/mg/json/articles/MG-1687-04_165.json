 {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[15]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[15]/text()[1]", "text": "Peuple, un nouveau Pasteur en ce jour te rameine"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[16]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[16]/text()[1]", "text": "La joye & les plaisirs, la santé, le printemps ;"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[17]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[17]/text()[1]", "text": "A ta felicité rien ne peut faire obstacle,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[18]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[18]/text()[1]", "text": "Tes pressantes langueurs demandoient un miracle ;"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[19]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[19]/text()[1]", "text": "Viens voir ce grand Prelat, & reverer ses loix,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[20]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[20]/text()[1]", "text": "Viens voir mille vertus briller sur son visage,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[21]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[21]/text()[1]", "text": "Et ton bonheur certain dans l’auguste assemblage"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[22]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[22]/text()[1]", "text": " De cent miracles à la fois."}], "attributes": {"rend": "i"}}]}]}]}