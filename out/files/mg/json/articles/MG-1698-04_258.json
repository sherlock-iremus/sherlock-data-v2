{"id:": "/div[1]", "tag": "div", "attributes": {"type": "article", "{http://www.w3.org/XML/1998/namespace}id": "MG-1698-04_258", "resp": "angele"}, "children": [{"id": "/div[1]/head[1]", "tag": "head", "children": [{"id": "/div[1]/head[1]/text()[1]", "text": "Air nouveau"}]}, {"id": "/div[1]/bibl[1]", "tag": "bibl", "children": [{"id": "/div[1]/bibl[1]/title[1]", "tag": "title", "children": [{"id": "/div[1]/bibl[1]/title[1]/text()[1]", "text": "Mercure galant"}]}, {"id": "/div[1]/bibl[1]/text()[1]", "text": ", avril 1698 [tome 4], p. 258-259."}]}, {"id": "/div[1]/p[1]", "tag": "p", "children": [{"id": "/div[1]/p[1]/text()[1]", "text": "L'Air que je vous envoye gravé, & dont vous allez lire les paroles a esté fait par un fort habile Musicien."}]}, {"id": "/div[1]/quote[1]", "tag": "quote", "children": [{"id": "/div[1]/quote[1]/label[1]", "tag": "label", "children": [{"id": "/div[1]/quote[1]/label[1]/text()[1]", "text": "AIR NOUVEAU."}]}, {"id": "/div[1]/quote[1]/note[1]", "tag": "note", "children": [{"id": "/div[1]/quote[1]/note[1]/hi[1]", "tag": "hi", "children": [{"id": "/div[1]/quote[1]/note[1]/hi[1]/text()[1]", "text": "Avis pour placer les Figures"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/note[1]/text()[1]", "text": " : l’Air qui commence par "}, {"id": "/div[1]/quote[1]/note[1]/hi[2]", "tag": "hi", "children": [{"id": "/div[1]/quote[1]/note[1]/hi[2]/text()[1]", "text": "J'ay vû perir cet aimable, &."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/note[1]/text()[2]", "text": ", doit regarder la page 258."}], "attributes": {"resp": "author", "place": "margin"}}, {"id": "/div[1]/quote[1]/lg[1]", "tag": "lg", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[1]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[1]/text()[1]", "text": "J'ay vû perir cet aimable feüillage,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[2]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[2]/text()[1]", "text": "Que j'ay v