ildren": [{"id": "/div[1]/quote[1]/lg[1]/l[6]/text()[1]", "text": "Veut qu’à son tour Sapho ressente ses bienfaits,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[7]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[7]/text()[1]", "text": "Et qu’enfin sous ses Loix, d’heureuses destinées"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[8]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[8]/text()[1]", "text": "Achevent de filer ses illustres années."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[9]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[9]/text()[1]", "text": "C’est-là, Muses, le soin qui doit vous réveiller,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[10]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[10]/text()[1]", "text": "Il vous seroit honteux de ne pas travailler,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[11]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[11]/text()[1]", "text": "Lors que LOUIS courant au devant du mérite,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[12]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[12]/text()[1]", "text": "Jette un si doux regard sur vostre Favorite."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[13]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[13]/text()[1]", "text": "Muses, vous le sçavez, elle vivoit chez nous"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[14]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[14]/text()[1]", "text": "Contente de soy-mesme, & contente de vous,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[15]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[15]/text()[1]", "text": "De la foule & du bruit dés longtemps séparée,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[16]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[16]/text()[1]", "text": "Et malgré cent vertus voulant estre ignorée ;"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[17]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[17]/text()[1]", "text": "Mais LOUIS, ce grand Roy, qui pourroit le tromper ?"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[18]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[18]/text()[1]", "text": "Est-il quelque vertu qui luy puisse échaper,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[19]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[19]/text()[1]", "text": "Et se sauver enfin de la main libérale"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[20]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[20]/text()[1]", "text": "Qui peint si bien les traits de son Ame Royale ?"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[21]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[21]/text()[1]", "text": "Non, Sapho vainement se cachoit au grand jour,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[22]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[22]/text()[1]", "text": "Il n’est rien que LOUIS ne distingue à son tour ;"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[23]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[23]/text()[1]", "text": "Mais bien que de ses dons à jamais honorée,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[24]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[24]/text()[1]", "text": "De l’ardeur la plus vive elle soit penétrée,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[25]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[25]/text()[1]", "text": "C’est de ce Roy si grand l’accueil remply d’attraits,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[26]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[26]/text()[1]", "text": "Et ses discours encor plus doux que ses bienfaits,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[27]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[27]/text()[1]", "text": "Qui comblerent Sapho d’une joye immortelle ;"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[28]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[28]/text()[1]", "text": "Et tout l’or du Pactole eust esté moins pour elle."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[29]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[29]/text()[1]", "text": "Mille autres, il est vray, de vos chers Nourrissons"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[30]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[30]/text()[1]", "text": "Favorisoient jadis les aimables Chansons,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[31]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[31]/text()[1]", "text": "Et l’on entend encor les bords de l’Hipocrene,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[32]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[32]/text()[1]", "text": "Retentir au grand nom d’Auguste, ou de Mecene."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[33]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[33]/text()[1]", "text": "Mais un Roy qui prévient jusques à vos desirs,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[34]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[34]/text()[1]", "text": "Qui du bonheur d’autruy fait ses premiers plaisirs,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[35]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[35]/text()[1]", "text": "Qui joint à ses présens une bonté supréme"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[36]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[36]/text()[1]", "text": "De l’air dont Jupiter les donneroit luy-mesme,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[37]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[37]/text()[1]", "text": "Et qui malgré l’éclat dont il est revétu,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[38]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[38]/text()[1]", "text": "Jusqu’au sein des Deserts va chercher la Vertu ;"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[39]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[39]/text()[1]", "text": "Ce n’est qu’en LOUIS seul, Muses, qu’on le rencontre,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[40]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[40]/text()[1]", "text": "Et ce n’est point ailleurs que vostre main le montre."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[41]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[41]/text()[1]", "text": "Peignez donc ce Héros, & ne peignez plus rien,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[42]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[42]/text()[1]", "text": "C’est Luy-seul qui sçait l’Art de bien faire du bien."}], "attributes": {"rend": "i"}}]}]}]}