: [{"id": "/div[1]/quote[1]/lg[1]/l[53]/text()[1]", "text": "Nous avions, il est vray, l’illustre des Houllieres,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[54]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[54]/text()[1]", "text": "Qui dans ce rude stile eut de douces manieres,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[55]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[55]/text()[1]", "text": "Et sans faire un faux pas dans ce champ raboteux,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[56]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[56]/text()[1]", "text": "Sçeut donner de la grace à tous ses Vers boiteux."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[57]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[57]/text()[1]", "text": "Je ne puis imiter son rare caractére,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[58]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[58]/text()[1]", "text": "Il m’apprend beaucoup moins à parler qu’à me taire,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[59]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[59]/text()[1]", "text": "Je n’en ay ny l’esprit ny l’inclination,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[60]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[60]/text()[1]", "text": "Et vous m’offrez en vain vostre protection."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[61]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[61]/text()[1]", "text": "Je trouve cependant cette maniere aisée,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[62]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[62]/text()[1]", "text": "On se laisse tomber où tombe la pensée,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[63]", "tag":