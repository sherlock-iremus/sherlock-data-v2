ildren": [{"id": "/div[1]/quote[1]/l[4]/text()[1]", "text": "Il s’en plaignit l’autre jour à sa Mere."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/l[5]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[5]/text()[1]", "text": "Elle luy dit ; mon Fils, chacun a son defaut."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/l[6]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[6]/text()[1]", "text": "Cette vertu pourtant a fait tomber sur elle"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/l[7]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[7]/text()[1]", "text": "L’heureux choix dont la gloire estoit deuë à son zele,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/l[8]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[8]/text()[1]", "text": "Choix qui mit en ses mains le Charme de la Cour,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/l[9]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[9]/text()[1]", "text": " Cette Princesse sans seconde,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/l[10]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[10]/text()[1]", "text": " Dont les yeux pourront bien un jour"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/l[11]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[11]/text()[1]", "text": "Te soûmettre le Fils du plus grand Roy du monde."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/l[12]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[12]/text()[1]", "text": "Quant à la Mareschale, en vain de la toucher"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/l[13]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[13]/text()[1]", "text": " Tu crois l’avantage possible."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/l[14]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[14]/text()[1]", "text": "  Son cœur est un rocher"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/l[15]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[15]/text()[1]", "text": " Qui fut toûjours inaccessible."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/l[16]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[16]/text()[1]", "text": "L’Amour à ce disours luy répondit ; Et bien"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/l[17]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[17]/text()[1]", "text": "Je consens qu’elle soit insensible à ma flame,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/l[18]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[18]/text()[1]", "text": " Mais qu’elle aille du tout au rien,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/l[19]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[19]/text()[1]", "text": "Je ne le puis souffrir ; tout au moins que son ame"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/l[20]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[20]/text()[1]", "text": "  Soit tendre à la pitié,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/l[21]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[21]/text()[1]", "text": "  Puis qu’on le peut sans blâme."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/l[22]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[22]/text()[1]", "text": "Je suis de son avis, & j’espere, Madame,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/l[23]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[23]/text()[1]", "text": "  Que j’auray sur ce pié"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/l[24]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/l[24]/text()[1]", "text": " Quelque part à vostre amitié."}], "attributes": {"rend": "i"}}]}]}