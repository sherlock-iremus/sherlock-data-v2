: {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[23]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[23]/text()[1]", "text": " Est que soigneusement je veille"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[24]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[24]/text()[1]", "text": "A vous donner des Eaux qui vous fassent du bien,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[25]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[25]/text()[1]", "text": " Sans quoy je compteray pour rien"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[26]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[26]/text()[1]", "text": "Tous mes autres succés dont on chante merveille ;"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[27]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[27]/text()[1]", "text": " Et si je réüssis à remplir ce devoir,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[28]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[28]/text()[1]", "text": " J’en seray trop récompensée"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[29]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[29]/text()[1]", "text": "Par un accroissement de ma gloire passée,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[30]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[30]/text()[1]", "text": " Que tout l’Univers va sçavoir,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[31]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[31]/text()[1]", "text": "Et parce que j’auray toûjours dans la pensée"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[32]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[32]/text()[1]", "text": "Le plaisir que je sens à présent de vous voir."}], "attributes": {"rend": "i"}}]}]}]}