: [{"id": "/div[1]/quote[1]/lg[1]/l[2]/text()[1]", "text": "S’arresta pour oüir les accens langoureux"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[3]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[3]/text()[1]", "text": "D’un jeune Rossignol, mais des plus amoureux,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[4]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[4]/text()[1]", "text": "Qui se plaignoit ainsi des froideurs de sa Belle."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[1]/l[5]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[5]/text()[1]", "text": "Philomele, pourquoy dédaignez-vous mes vœux ?"}]}, {"id": "/div[1]/quote[1]/lg[1]/l[6]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[6]/text()[1]", "text": " Pour vous seule mon cœur soûpire ;"}]}, {"id": "/div[1]/quote[1]/lg[1]/l[7]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[7]/text()[1]", "text": "Cruelle, voulez-vous que je sois malheureux"}]}, {"id": "/div[1]/quote[1]/lg[1]/l[8]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[8]/text()[1]", "text": " Au dela de ce qu’on peut dire ?"}]}, {"id": "/div[1]/quote[1]/lg[1]/l[9]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[9]/text()[1]", "text": "Ah, vous ne sçavez pas à quel point un Amant,"}]}, {"id": "/div[1]/quote[1]/lg[1]/l[10]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[10]/text()[1]", "text": " Lors qu’il aime parfaitement,"}]}, {"id": "/div[1]/quote[1]/lg[1]/l[11]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[11]/text()[1]", "text": " Souffre d’un dédaigneux silence."}]}, {"id": "/div[1]/quote[1]/lg[1]/l[12]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[12]/text()[1]", "text": "Ayez d’autres rigueurs, insultez ma langueur,"}]}, {"id": "/div[1]/quote[1]/lg[1]/l[13]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[1]/l[13]/text()[1]", "text": "Vous ne sçauriez