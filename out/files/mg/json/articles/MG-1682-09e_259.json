ildren": [{"id": "/div[1]/quote[1]/lg[2]/l[18]/text()[1]", "text": "  D’une Onde claire & pure,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[2]/l[19]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[19]/text()[1]", "text": "Pour mieux s’abandonner à ses cruels transports,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[2]/l[20]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[20]/text()[1]", "text": " Et déplorer son avanture."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[2]/l[21]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[21]/text()[1]", "text": "A ses gémissemens l’Hirondelle sent bien"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[2]/l[22]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[22]/text()[1]", "text": "  Qu’elle seroit d’humeur à faire"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[2]/l[23]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[23]/text()[1]", "text": "Avec un tel Amant un voyage à Cythere."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[2]/l[24]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[24]/text()[1]", "text": "Le plumage, la voix, la taille, & le maintien,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[2]/l[25]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[25]/text()[1]", "text": "Tout dans ce Malheureux luy paroist admirable."}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[2]/l[26]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[26]/text()[1]", "text": " Ah ! "}, {"id": "/div[1]/quote[1]/lg[2]/l[26]/hi[1]", "tag": "hi", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[26]/hi[1]/text()[1]", "text": "disoit-elle,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[2]/l[26]/text()[2]", "text": " il n’est pas raisonnable"}]}, {"id": "/div[1]/quote[1]/lg[2]/l[27]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[27]/text()[1]", "text": " Que cette Belle vainement"}]}, {"id": "/div[1]/quote[1]/lg[2]/l[28]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[28]/text()[1]", "text": " Fasse soûpirer cet Amant."}]}, {"id": "/div[1]/quote[1]/lg[2]/l[29]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[29]/text()[1]", "text": " Ah ! sans-doute elle est insensible,"}]}, {"id": "/div[1]/quote[1]/lg[2]/l[30]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[30]/text()[1]", "text": "  Puis qu’il n’est pas possible"}]}, {"id": "/div[1]/quote[1]/lg[2]/l[31]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[31]/text()[1]", "text": "De résister aux merveilleux amas"}]}, {"id": "/div[1]/quote[1]/lg[2]/l[32]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[32]/text()[1]", "text": "  De ses charmans appas."}]}, {"id": "/div[1]/quote[1]/lg[2]/l[33]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[33]/text()[1]", "text": " Pour moy, qui me rens attentive"}]}, {"id": "/div[1]/quote[1]/lg[2]/l[34]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[34]/text()[1]", "text": " Jusques à la moindre action"}]}, {"id": "/div[1]/quote[1]/lg[2]/l[35]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[35]/text()[1]", "text": "Que fait le Rossignol icy sur cette Rive,"}]}, {"id": "/div[1]/quote[1]/lg[2]/l[36]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[36]/text()[1]", "text": " Je sens beaucoup d’émotion,"}]}, {"id": "/div[1]/quote[1]/lg[2]/l[37]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[37]/text()[1]", "text": " Et par là je vois que je l’aime."}]}, {"id": "/div[1]/quote[1]/lg[2]/l[38]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[38]/text()[1]", "text": "Dis-moy de grace, Amour, l’aurois-tu résolu,"}]}, {"id": "/div[1]/quote[1]/lg[2]/l[39]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[39]/text()[1]", "text": "Que je luy parlerois en faveur de moy-mesme,"}]}, {"id": "/div[1]/quote[1]/lg[2]/l[40]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[40]/text()[1]", "text": "Quand pour un autre Objet son ardeur est extréme ?"}]}, {"id": "/div[1]/quote[1]/lg[2]/l[41]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[2]/l[41]/text()[1]", "text": "Fais-luy connoistre au moins qu’ainsi tu l’as voulu."}]}]}, {"id": "/div[1]/quote[1]/ab[2]", "tag": "ab", "children": [{"id": "/div[1]/quote[1]/ab[2]/text()[1]", "text": "***"}], "attributes": {"type": "ornament"}}, {"id": "/div[1]/quote[1]/lg[3]", "tag": "lg", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[1]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[1]/text()[1]", "text": "Dans ce doux entretien que se fait l’Hirondelle,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[3]/l[2]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[2]/text()[1]", "text": "Elle se voit dans l’Onde, & se trouve assez belle"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[3]/l[3]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[3]/text()[1]", "text": "Pour plaire au Rossignol. Elle avance ses pas,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[3]/l[4]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[4]/text()[1]", "text": "Etalant de son mieux le peu qu’elle a d’appas,"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[3]/l[5]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[5]/hi[1]", "tag": "hi", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[5]/hi[1]/text()[1]", "text": "Et luy dit"}], "attributes": {"rend": "i"}}, {"id": "/div[1]/quote[1]/lg[3]/l[5]/text()[1]", "text": " J’écoutois cette plainte amoureuse"}]}, {"id": "/div[1]/quote[1]/lg[3]/l[6]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[6]/text()[1]", "text": " Que d’une voix mélodieuse,"}]}, {"id": "/div[1]/quote[1]/lg[3]/l[7]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[7]/text()[1]", "text": "Oyseau trop malheureux, vous exposez icy."}]}, {"id": "/div[1]/quote[1]/lg[3]/l[8]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[8]/text()[1]", "text": " Je sçay quel est vostre soucy ;"}]}, {"id": "/div[1]/quote[1]/lg[3]/l[9]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[9]/text()[1]", "text": " Amour par tout dans son Empire,"}]}, {"id": "/div[1]/quote[1]/lg[3]/l[10]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[10]/text()[1]", "text": "Depuis que l’on y vit, & que l’on y soûpire,"}]}, {"id": "/div[1]/quote[1]/lg[3]/l[11]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[11]/text()[1]", "text": "N’a jamais veu d’Amans plus à plaindre que vous."}]}, {"id": "/div[1]/quote[1]/lg[3]/l[12]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[12]/text()[1]", "text": "Philomele, il est vray, chante bien, est bien faite,"}]}, {"id": "/div[1]/quote[1]/lg[3]/l[13]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[13]/text()[1]", "text": "Et pourroit vous causer le destin le plus doux ;"}]}, {"id": "/div[1]/quote[1]/lg[3]/l[14]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[14]/text()[1]", "text": " Mesme elle est digne qu’on la traite"}]}, {"id": "/div[1]/quote[1]/lg[3]/l[15]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[15]/text()[1]", "text": "  De la Vénus des Oyseaux."}]}, {"id": "/div[1]/quote[1]/lg[3]/l[16]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[16]/text()[1]", "text": "J’en puis parler ainsi ; depuis peu je l’ay veuë"}]}, {"id": "/div[1]/quote[1]/lg[3]/l[17]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[17]/text()[1]", "text": "Qui se desaltéroit au courant de ces eaux."}]}, {"id": "/div[1]/quote[1]/lg[3]/l[18]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[18]/text()[1]", "text": "Un si charmant Objet me donnant dans la veuë,"}]}, {"id": "/div[1]/quote[1]/lg[3]/l[19]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[19]/text()[1]", "text": "Je faisois mon plaisir de la bien contempler."}]}, {"id": "/div[1]/quote[1]/lg[3]/l[20]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[20]/text()[1]", "text": "  A ne vous rien celer,"}]}, {"id": "/div[1]/quote[1]/lg[3]/l[21]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[21]/text()[1]", "text": "  J’en fus toute surprise ;"}]}, {"id": "/div[1]/quote[1]/lg[3]/l[22]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[22]/text()[1]", "text": "Si mon Sexe changeoit, j’en aurois l’ame éprise."}]}, {"id": "/div[1]/quote[1]/lg[3]/l[23]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[23]/text()[1]", "text": "Mais que dis-je ! elle auroit mille fois plus d’appas,"}]}, {"id": "/div[1]/quote[1]/lg[3]/l[24]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[24]/text()[1]", "text": "Je ne pourrois l’aimer, elle ne m’aimant pas."}]}, {"id": "/div[1]/quote[1]/lg[3]/l[25]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[25]/text()[1]", "text": "Ah ! gentil Rossignol, ce seroit grand dommage"}]}, {"id": "/div[1]/quote[1]/lg[3]/l[26]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[26]/text()[1]", "text": "De consumer le printemps de vostre âge"}]}, {"id": "/div[1]/quote[1]/lg[3]/l[27]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[27]/text()[1]", "text": " Parmy les sanglots & les pleurs,"}]}, {"id": "/div[1]/quote[1]/lg[3]/l[28]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[28]/text()[1]", "text": "Lors qu’ailleurs vostre amour peut trouver des douceurs."}]}, {"id": "/div[1]/quote[1]/lg[3]/l[29]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[29]/text()[1]", "text": "Il vaut mieux les gouster, l’âge vous y convie."}]}, {"id": "/div[1]/quote[1]/lg[3]/l[30]", "tag": "l", "children": [{"id": "/div[1]/quote[1]/lg[3]/l[30]/text()[1]", "text": " Voyez-vous, on n’a dans la vie"}]}, {"id": "/div[1]/quote[1]/lg[3]/l[31]", "tag": "l", "children": [{"id": 